<script>		
	var mamTimeout = false;
	var mamHeader = $('.mam-header');

	function triggerMamTimeout() {
		mamTimeout = setTimeout(() => {
			toggleMam();		
			triggerMamTimeout();
		}, 5000);
	}

	function toggleMam() {
		if(mamHeader.hasClass('mam-header-switch')) {
			mamHeader.removeClass('mam-header-switch');
		} else {
			mamHeader.addClass('mam-header-switch');
		}
	}

	$('.mam-dot').on('click', function(e) {
		clearTimeout(mamTimeout);
		
		if(!$(this).hasClass('mam-dot-first') != $('.mam-header').hasClass('mam-header-switch')) {
			toggleMam();
		}
	});

	triggerMamTimeout();
</script>
