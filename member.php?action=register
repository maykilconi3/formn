<script>		
	var mamTimeoutFooter = false;
	var mamFooter = $('.mam-footer');

	function triggerMamTimeoutFooter() {
		mamTimeoutFooter = setTimeout(() => {
			toggleMamFooter();		
			triggerMamTimeoutFooter();
		}, 5000);
	}

	function toggleMamFooter() {
		if(mamFooter.hasClass('mam-header-switch')) {
			mamFooter.removeClass('mam-header-switch');
		} else {
			mamFooter.addClass('mam-header-switch');
		}
	}

	$('.mam-dot').on('click', function(e) {
		clearTimeout(mamTimeoutFooter);
		
		if(!$(this).hasClass('mam-dot-first') != $('.mam-footer').hasClass('mam-header-switch')) {
			toggleMamFooter();
		}
	});

	triggerMamTimeoutFooter();
</script>
