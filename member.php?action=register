<script>

	if(Cookie.get('mybb_bgcolor')) {
		document.body.style.backgroundColor = Cookie.get('mybb_bgcolor');	
	}

function colorBlack() {
    document.body.style.backgroundColor = '#000';
	Cookie.set('mybb_bgcolor', "#000");
}

function colorGrey() {
    document.body.style.backgroundColor = '#343434';
	Cookie.set('mybb_bgcolor', "#343434");
}
	
function colorDefault() {
    document.body.style.backgroundColor = '#282828';
	Cookie.unset('mybb_bgcolor');
}
	
</script>


